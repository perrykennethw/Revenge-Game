package {	import flash.display.MovieClip;		public class Wall extends MovieClip	{		public function Wall()		{			// constructor code			MovieClip(root).platforms.push(this);			//this.alpha = 0;		}				public function collidePlatform(object:MovieClip, level:MovieClip):void		{			collideFeet(object, level);			collideHead(object, level);			collideRight(object, level);			collideLeft(object, level);		}		public function collideFeet(object:MovieClip, level:MovieClip):void		{			if(this.hitTestObject(object.feet_mc) && object.vy > 0)			{					object.canJump = true;				object.vy = 0;				level.y = object.y - this.y;							}		}		public function collideHead(object:MovieClip, level:MovieClip):void		{			if(this.hitTestObject(object.head_mc))			{			level.y = object.y - object.height -   this.y-this.height-1;			object.vy = 0;						}		}		public function collideRight(object:MovieClip, level:MovieClip):void		{			if(this.hitTestObject(object.right_mc))			{				//MovieClip(root).fart.right = false;				MovieClip(root).player_mc.vx =0;				level.x = object.prevX;			}		}		public function collideLeft(object:MovieClip, level:MovieClip):void		{			if(this.hitTestObject(object.left_mc))			{			MovieClip(root).player_mc.vx =0;			level.x = object.prevX;						}		}			}}